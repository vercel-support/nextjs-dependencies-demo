{
    "version": 2,
    "name": "nextjs-dependencies-demo",
    "scope": "support-team",
    "builds": [
        { "src": "package.json", "use": "@now/next" },
        { "src": "api/**/*.js", "use": "@now/node" }
    ],
    "routes": [
      { "src": "/api/orders/(?<id>[^/]+)", "dest": "/api/orders/?id=$id" },
      { "src": "/api/(.*)", "dest": "/api/$1" }
    ]
}
